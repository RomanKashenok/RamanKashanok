<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".homework.hw6.Hw6MainActivity"
        android:padding="@dimen/dimen_9dp">

    <FrameLayout
            android:id="@+id/studentsListFragmentFrame"
            android:layout_height="match_parent"
            android:layout_width="@dimen/dimen_350dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <FrameLayout
            android:id="@+id/studentsRightPartFragmentFrame"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            app:layout_constraintStart_toEndOf="@id/studentsListFragmentFrame"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

</android.support.constraint.ConstraintLayout>